
        <table border='1'>
            <tr>
                <th>Name</th>
                <th>Age</th>
                <th>Occupation</th>
                <th>Company</th>
            </tr>
            {{#each data}}
            {{#if display}}
            <tr>
                <td>{{name}}</td>
                <td>{{age}}</td>
                <td>{{occupation}}</td>
                <td>{{company}}</td>
            </tr>
            {{else}}
            <tr>
                <td colspan="4">{{name}} has protected their data</td>
            </tr>
            {{/if}}
            {{else}}
            <tr>
                <td colspan="4">Data not avaliable</td>
            </tr>
            {{/each}}
        </table>
    